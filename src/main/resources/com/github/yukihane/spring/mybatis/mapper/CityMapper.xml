<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.yukihane.spring.mybatis.mapper.CityMapper">
  <select id="findByState" resultType="com.github.yukihane.spring.mybatis.domain.City">
    select * from city where state = #{state}
  </select>
  <select id="selectCityById" resultType="com.github.yukihane.spring.mybatis.domain.City">
    select * from city where id = #{id}
  </select>
</mapper>